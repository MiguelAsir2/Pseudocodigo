Proceso Distancia_entre_puntos
	Definir x1,y1,x2,y2 como Reales;
	Escribir "Escriba la ordenada x1:";
	Leer x1;
	Escribir "Escriba la ordenada y1:";
	Leer y1;
	Escribir "Escriba la ordenada x2:";
	Leer x2;
	Escribir "Escriba la ordenada y2:";
	Leer y2;
	Escribir "La distancia entre los puntos ",x1,",",y1," y ",x2,",",y2," es de ",rc((x2-x1)^2+(y2-y1)^2);	
FinProceso
